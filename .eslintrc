{
    "extends": [
        "eslint:recommended",
        "react-app"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "semi": [
            1,
            "never"
        ],
        "no-mixed-spaces-and-tabs": [
            1
        ],
        "no-trailing-spaces": [
            1
        ],
        "no-console": [
            0
        ],
        "space-infix-ops": [
            1
        ],
        "quotes": [
            1,
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "no-unused-vars": 1,
        "one-var": [
            1,
            "never"
        ],
        "vars-on-top": 1,
        "no-undef": 1,
        "no-use-before-define": 1,
        "keyword-spacing": 1,
        "key-spacing": 1,
        "space-before-blocks": 1,
        "space-in-parens": 1,
        "indent": [
            1,
            4
        ],
        "camelcase": [
            1,
            {
                "properties": "always"
            }
        ],
        "comma-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "newline-after-var": [
            1,
            "always"
        ],
        "newline-before-return": 1,
        "object-curly-spacing": [
            1,
            "never"
        ],
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "eqeqeq": 1,
        "linebreak-style": [
            1,
            "unix"
        ]
    }
}